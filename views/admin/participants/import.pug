extends ../../layouts/admin

block content
  h1 Import Participants
  form(method='post',autocomplete='off')
    .form-group
      textarea.form-control(name='input')
      span.help-block ["name", "position", "company"] for each line
    p
      button.btn.btn-primary(type='submit')
        i.fa.fa-upload
        |  Import
      | &nbsp;
      a.btn.btn-default(href=event_path(event)) Cancel
